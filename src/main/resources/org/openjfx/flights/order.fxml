        <HBox prefHeight="100.0" prefWidth="200.0" GridPane.columnIndex="1" GridPane.rowIndex="4">
            <Region HBox.hgrow="ALWAYS" />
            <Button fx:id="odstraniButton" alignment="BOTTOM_RIGHT" contentDisplay="RIGHT" mnemonicParsing="false" text="Odstrani">
            <HBox.margin>
               <Insets right="20.0" />
            </HBox.margin></Button>
        </HBox>
      <opaqueInsets>
         <Insets />
      </opaqueInsets>
      <VBox.margin>
         <Insets left="20.0" />
      </VBox.margin>
    </GridPane>
</VBox>
