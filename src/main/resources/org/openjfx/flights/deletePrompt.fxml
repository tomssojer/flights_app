      <Label layoutX="45.0" layoutY="62.0" text="Ali ste prepričani da želite izbrisati rezervacijo?" />
      <GridPane alignment="CENTER" layoutX="42.0" layoutY="91.0" prefHeight="97.0" prefWidth="314.0">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Label fx:id="nameLabel" alignment="CENTER" style="-fx-alignment: center;" text="Ime in priimek" />
            <Label fx:id="locationLabel" prefHeight="16.0" prefWidth="162.0" styleClass="align-right" text="Destinacija" GridPane.columnIndex="1" />
            <Button fx:id="prekliciButton" mnemonicParsing="false" text="Prekliči" GridPane.rowIndex="1" />
            <Button fx:id="izbrisiButton" mnemonicParsing="false" styleClass="btn, btn-danger" text="Izbriši" GridPane.columnIndex="1" GridPane.rowIndex="1" />
         </children>
      </GridPane>
   </children>
</AnchorPane>
